WHEN PREP.PCO_LOAD.DESCRIPTION = '3_519_A000010541_OBC_LTE_TO_FIBRE_MAGRATION_DEALER_20220812' THEN 'MIGRATION LTE TO FIBRE'
WHEN PREP.PCO_LOAD.DESCRIPTION = '3_520_A000010545_OBC_MOBILE_TO_FIBRE_MAGRATION_DEALER_20220812' THEN 'MIGRATION MOBILE TO FIBRE'
WHEN PREP.PCO_LOAD.DESCRIPTION = '3_521_OBC_OPENSERVE_MIG_FIBRE_LEAD_VOICE_07_12_2022' THEN 'MIGRATION LEAD VOICE TO FIBRE'
WHEN PREP.PCO_LOAD.DESCRIPTION = '3_522_OBC_OPENSERVE_MIG_LEAD_DSL_07_12_2022' THEN 'MIGRATION DSL TO FIBRE'
WHEN PREP.PCO_LOAD.DESCRIPTION = '8_Telkom_20220804_Mix1' THEN 'Try Before you Buy'
WHEN PREP.PCO_LOAD.DESCRIPTION = '9_405_2022-08-20_POF_GAU' THEN 'PREORDER FIBRE'
WHEN PREP.PCO_LOAD.DESCRIPTION = '9_406_2022-08-20_POF_COASTAL' THEN 'PREORDER FIBRE'
WHEN PREP.PCO_LOAD.DESCRIPTION = '9_407_2022-08-20_POF_INTERIOR' THEN 'PREORDER FIBRE'
WHEN PREP.PCO_LOAD.DESCRIPTION = '9_443_2022-10-25_HomeOwnerPOA_20220929' THEN 'PREORDER FIBRE'
WHEN PREP.PCO_LOAD.DESCRIPTION = '9_466_PREORDERLIST_2022_11_07_1' THEN 'PREORDER FIBRE'
WHEN PREP.PCO_LOAD.DESCRIPTION = '9_467_PREORDERLIST_2022_11_07_2' THEN 'PREORDER FIBRE'
WHEN PREP.PCO_LOAD.DESCRIPTION = '9_487_PREORDERLIST_2022_11_22_1' THEN 'PREORDER FIBRE'
WHEN PREP.PCO_LOAD.DESCRIPTION = '9_488_PREORDERLIST_2022_11_22_2' THEN 'PREORDER FIBRE'
WHEN PREP.PCO_LOAD.DESCRIPTION = '9_510_PREORDERLIST_2022-12-02_1' THEN 'PREORDER FIBRE'
WHEN PREP.PCO_LOAD.DESCRIPTION = '9_511_PREORDERLIST_2022-12-02_2' THEN 'PREORDER FIBRE'
WHEN PREP.PCO_LOAD.DESCRIPTION = '9_Telkom_20220812_HomeOwner' THEN 'Telkom HomeOwner'
WHEN PREP.PCO_LOAD.DESCRIPTION = 'OBC_OPENSERVE_MIG_FIBRE_LEAD_DSL_20221010' THEN 'MIGRATION DSL TO FIBRE'
WHEN PREP.PCO_LOAD.DESCRIPTION = 'OBC_OPENSERVE_MIG_FIBRE_LEAD_DSL_GREY_20201010' THEN 'MIGRATION DSL GREY TO FIBRE'
WHEN PREP.PCO_LOAD.DESCRIPTION = 'OBC_OPENSERVE_MIG_FIBRE_LEAD_VOICE_20221010' THEN 'MIGRATION LEAD VOICE TO FIBRE'
WHEN PREP.PCO_LOAD.DESCRIPTION = 'OBC30DAYSFIXCOMPLEX20220506' THEN '30 DAYS FIX COMPLEX'
WHEN PREP.PCO_LOAD.DESCRIPTION = 'OBC30DAYSFIXCOMPLEX20220517' THEN '30 DAYS FIX COMPLEX'
WHEN PREP.PCO_LOAD.DESCRIPTION = 'OBC30DAYSFIXSTANDALONE20220517' THEN '30 DAYS STANDALONE'
WHEN PREP.PCO_LOAD.DESCRIPTION = 'OBC30DAYSFIXSTANDALONE220506' THEN '30 DAYS STANDALONE'
WHEN PREP.PCO_LOAD.DESCRIPTION = 'OBC30DAYSMOBILECOMPLEX20220506' THEN '30 DAYS MOBILE COMPLEX'
WHEN PREP.PCO_LOAD.DESCRIPTION = 'OBC30DAYSMOBILECOMPLEX20220517' THEN '30 DAYS MOBILE COMPLEX'
WHEN PREP.PCO_LOAD.DESCRIPTION = 'OBC30DAYSMOBILESTANDALONE20220506' THEN '30 DAYS MOBILE STANDALONE'
WHEN PREP.PCO_LOAD.DESCRIPTION = 'OBC30DAYSMOBILESTANDALONE20220517' THEN '30 DAYS MOBILE STANDALONE'
WHEN PREP.PCO_LOAD.DESCRIPTION = 'SocialMedia_FacebookLiveList' THEN 'FaceBook Fibre Data'
WHEN PREP.PCO_LOAD.DESCRIPTION = 'SocialMedia_ManualUpload1' THEN 'Website Leads'
WHEN PREP.PCO_LOAD.DESCRIPTION = 'SocialMediaLeadsRetryConnexList9680' THEN 'Website Leads'
WHEN PREP.PCO_LOAD.DESCRIPTION = 'Telkom_LTEDataPlanSocialMediaManualList_20220715' THEN 'Website Leads'
WHEN PREP.PCO_LOAD.DESCRIPTION = 'Telkom_SocialMediaManualList_20220725' THEN 'Website Leads'
WHEN PREP.PCO_LOAD.DESCRIPTION = 'Telkom_SocialMediaManualList_Nov2022' THEN 'Website Leads'
WHEN PREP.PCO_LOAD.DESCRIPTION = 'TelkomLTEDataPlanSocialMediaManualList_20220719' THEN 'Website Leads'
WHEN PREP.PCO_LOAD.DESCRIPTION = 'TelkomSocialMediaLeads_Aug2022' THEN 'Website Leads'
WHEN PREP.PCO_LOAD.DESCRIPTION = 'USSDLiveList' THEN 'Website Leads'
WHEN PREP.PCO_LOAD.DESCRIPTION = 'Vasco_Telkom_ReconList_20220729' THEN 'Website Leads'
WHEN PREP.PCO_LOAD.DESCRIPTION = 'Website__ManualUpload' THEN 'Website Leads'
WHEN PREP.PCO_LOAD.DESCRIPTION = 'WebsiteGetDataFormLiveList' THEN 'Website Leads'
WHEN PREP.PCO_LOAD.DESCRIPTION = 'WebsiteGetFibreLiveList' THEN 'Website Leads'
ELSE PREP.PCO_LOAD.DESCRIPTION END AS [Load Description],

Convert(date,PREP.PCO_OUTBOUNDQUEUE.RDATE) as [Recieved Date],
Convert(date,PREP.PCO_OUTBOUNDQUEUE.FIRSTHANDLINGDATE) as [First Handling Date],
Convert(Date,PREP.PCO_OUTBOUNDQUEUE.LASTHANDLINGDATE) as [Last Handling Date], RIGHT('00' + CONVERT(VARCHAR(10),
DATEDIFF(second, PREP.PCO_OUTBOUNDQUEUE.RDATE, PREP.PCO_OUTBOUNDQUEUE.FIRSTHANDLINGDATE) / 3600), 2) + ':' + RIGHT('00' + CONVERT(VARCHAR(2),
DATEDIFF(second, PREP.PCO_OUTBOUNDQUEUE.RDATE, PREP.PCO_OUTBOUNDQUEUE.FIRSTHANDLINGDATE) % 3600 / 60), 2) + ':' + RIGHT('00' + CONVERT(VARCHAR(2),
DATEDIFF(second, PREP.PCO_OUTBOUNDQUEUE.RDATE, PREP.PCO_OUTBOUNDQUEUE.FIRSTHANDLINGDATE) % 60), 2) AS TimeToTouch,
PREP.PCO_OUTBOUNDQUEUE.DAILYCOUNTER,
PREP.PCO_OUTBOUNDQUEUE.TOTALCOUNTER,
PREP.PCO_OUTBOUNDQUEUE.BUSYSIGNALCOUNTER,
PREP.PCO_OUTBOUNDQUEUE.NOANSWERCOUNTER,
PREP.PCO_OUTBOUNDQUEUE.ANSWERMACHINECOUNTER,
PREP.PCO_OUTBOUNDQUEUE.FAXCOUNTER,
PREP.PCO_OUTBOUNDQUEUE.INVGENREASONCOUNTER,
PREP.PCO_OUTBOUNDQUEUE.PRIORITY,
PREP.PCO_OUTBOUNDQUEUE.CAPTURINGAGENT,
PREP.PCO_OUTBOUNDQUEUE.COMMENTS
FROM   dbo.StatusCodes
INNER JOIN PREP.PCO_OUTBOUNDQUEUE ON dbo.StatusCodes.StatusId = PREP.PCO_OUTBOUNDQUEUE.STATUS
LEFT OUTER JOIN PREP.PCO_AGENT
INNER JOIN PREP.PCO_LOGINAGENT ON PREP.PCO_AGENT.ID = PREP.PCO_LOGINAGENT.AGENTID ON PREP.PCO_OUTBOUNDQUEUE.LASTAGENT = PREP.PCO_LOGINAGENT.LOGIN 
LEFT OUTER JOIN PREP.PCO_LOAD ON PREP.PCO_OUTBOUNDQUEUE.LOADID = PREP.PCO_LOAD.LOADID AND PREP.PCO_OUTBOUNDQUEUE.SERVICEID = PREP.PCO_LOAD.SERVICEID 
LEFT OUTER JOIN PREP.PCO_OUTBOUNDSERVICE ON PREP.PCO_OUTBOUNDQUEUE.SERVICEID = PREP.PCO_OUTBOUNDSERVICE.ID 
LEFT OUTER JOIN dbo.OutboundQCodeByServiceID ON PREP.PCO_OUTBOUNDQUEUE.SERVICEID = dbo.OutboundQCodeByServiceID.SERVICEID AND PREP.PCO_OUTBOUNDQUEUE.LASTQCODE = dbo.OutboundQCodeByServiceID.QCODE
WHERE PREP.PCO_OUTBOUNDQUEUE.SERVICEID in (1101, 1111, 1110, 1112, 1127, 1128, 1130, 1133, 1134, 1135, 1137, 1138, 1123, 1129, 1131, 1132, 1136) and
PREP.PCO_OUTBOUNDQUEUE.RDATE >= '2022-01-01 00:00:00' and PREP.PCO_OUTBOUNDQUEUE.RDATE <= '2023-01-01 00:00:00'
ORDER BY SOURCEID
